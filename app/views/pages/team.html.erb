<%= stylesheet_link_tag "team", media: "all" %>
<div class="container mt-5">
  <section id="team">
    <div class="text-center mb-5">
      <h1 class="mb-3">Notre Équipe</h1>
      <p class="lead">Découvrez les membres talentueux qui font le succès de notre entreprise.</p>
    </div>

    <div class="row">
      <div class="col-md-4">
        <div class="card text-center">
          <%= image_tag "1689169425140.jpg", class: "card-img-top", alt: "Stéphane" %>
          <div class="card-body">
            <h5 class="card-title">Stéphane</h5>
            <p class="card-text">Description courte du rôle ou de la spécialité du membre.</p>
          </div>
        </div>
      </div>

      <div class="col-md-4">
        <div class="card text-center">
          <%= image_tag "83050.jpg", class: "card-img-top", alt: "Zaïneb" %>
          <div class="card-body">
            <h5 class="card-title">Zaïneb</h5>
            <p class="card-text">Description courte du rôle ou de la spécialité du membre.</p>
          </div>
        </div>
      </div>

      <div class="col-md-4">
        <div class="card text-center">
          <%= image_tag "photonora.JPG", class: "card-img-top", alt: "Nora" %>
          <div class="card-body">
            <h5 class="card-title">Nora</h5>
            <p class="card-text">Description courte du rôle ou de la spécialité du membre.</p>
          </div>
        </div>
      </div>
    </div>
  </section>

  <section id="contact">
    <div class="contact-container">
      <%= render 'contact_form/contact_form' %>
      <div class="contact-right">
        <%= image_tag "contact-us1.jpg", alt: "Logo", class: "contact-logo img-fluid" %>
      </div>
    </div>
  </section>
</div>
